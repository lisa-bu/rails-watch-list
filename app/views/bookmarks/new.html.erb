<div class="container">
  <h1>Add a Movie</h1>
  <%= simple_form_for [@list, @bookmark] do |f| %>
    <%= f.input :comment %>
    <%= f.association :movie %>
    <%= f.submit 'Submit', class: 'btn btn-info' %>
  <% end %>
  <%= link_to "Go back", list_path(@list), class: "btn btn-info mt-2"  %>
</div>
